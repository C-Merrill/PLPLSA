<%include ../layout%>
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">Articles</h1>
      <ol class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/investors">Investors</a></li>
        <li class="active">Press Releases</li>
      </ol>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-3">
      <% for(var i = 0; i < articles.length;i++) {%>
      <h5><a href='/investors/press/<%=i%>'><%=articles[i].title%></a></h5>
      <!-- links -->
      <% } %>
    </div>
    <div class="col-sm-9">
      <%if(preview){%>
        <%if(id==0){%>
          <div class='section-head-solid'>
              <h5>Latest Press Release</h5>
              <embed src='/press_releases/<%=articles[id].article %>' width="100%" height="800px" />
          </div>
        <% } else { %>
          <embed src="/press_releases/<%=articles[id].article %>" width="100%" height="800px" />
        <% } %>
      <% } %>
    </div>
  </div>
</div>
<%include ../foot%>
