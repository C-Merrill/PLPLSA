<h2>Senteeko Estate</h2>
<h4>Landscape</h4>
<% for(var i=0; i<landscapeImgs.length; i++) { %>
<%  if(i%3 === 0) { %>
    <div class='row gallery-row'>
<%  } %>
    <div class='col-sm-4 gallery-item'>
        <a href='/images/Estate/<%=landscapeImgs[i]%>'><img src='/images/Estate/thumbs/<%=landscapeImgs[i]%>' /></a>
    </div>
<%  if(i%3 === 2 || i === landscapeImgs.length-1) { %>
    </div>
<%  } %>
<% } %>
