<%include layout%>
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">Articles</h1>
      <ol class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/articles">Articles</a></li>
        <li class="active">Shareholder Updates</li>
      </ol>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-3">
      <% for(var i = 0; i < articles.length;i++) {%>
      <h5><a href='/articles/shareholder/<%=i%>'><%=articles[i].title%></a></h5>
      <!-- links -->
      <% } %>
    </div>
    <div class="col-sm-9">
      <%if(preview){%>
      <embed src="/shareholder_updates/<%=articles[id].article %>" width="100%" height="800px" />
      <% } %>
    </div>
  </div>
</div>
<%include foot%>